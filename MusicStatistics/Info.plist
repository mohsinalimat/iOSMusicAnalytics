			<dict>
				<key>version</key>
				<string>${APP_VERSION}</string>
			</dict>
		</dict>
        <dict>
            <key>UIApplicationShortcutItemIconFile</key>
            <string>analyticsIcon</string>
            <key>UIApplicationShortcutItemSubtitle</key>
            <string></string>
